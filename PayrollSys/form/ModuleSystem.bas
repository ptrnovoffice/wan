Public Function CheckFormStatus(Myform As Form) As Boolean
    Dim objForm As Form
    Dim FlgLoaded As Boolean
    Dim FlgShown As Boolean
    FlgLoaded = False
    FlgShown = False
    For Each objForm In VB.Forms
        If (Trim(objForm.Name) = Trim(Myform.Name)) Then
            FlgLoaded = True
            If objForm.Visible Then
                 FlgShown = True
            End If
            Exit For
        End If
    Next
    'MsgBox "Load Status: " & FlgLoaded & vbCrLf & "Show Status:" & FlgShown
    CheckFormStatus = FlgLoaded
End Function


'Converts the Points measurement argument to screen Pixels
'ARGUMENTS:
'   lngPointsValue  := number of Points to convert to Pixels
'   blnX            := TRUE to calculate a horizontal screen measurement,
'                       FALSE for a vertical measurement

Function Get_PixelsFromPoints(ByVal lngPointsValue As Long, blnX As Boolean) As Long
Dim hWindow As Long
Dim hDeviceContext As Long
Dim lngDPI As Long
Dim lngPropertyIndex As Long
Dim intReleaseSuccess As Integer

Const LOGPIXELSX = 88
Const LOGPIXELSY = 90

On Error GoTo ErrorH

    'return handle to the active window
    hWindow = GetForegroundWindow()
    'return a Device Context for the window
    hDeviceContext = GetDC(hWindow)
    
    Select Case blnX
        Case True: lngPropertyIndex = LOGPIXELSX
        Case Else: lngPropertyIndex = LOGPIXELSY
    End Select
    
    'return Pixels per Inch property of the Device Context
    lngDPI = GetDeviceCaps(hDeviceContext, lngPropertyIndex)
    'release the Device Context
    intReleaseSuccess = ReleaseDC(hWindow, hDeviceContext)
    '72 Points per Inch
    Get_PixelsFromPoints = (lngPointsValue * lngDPI) \ 72

Finish:
    Exit Function
ErrorH:
    Resume Finish
    
End Function
