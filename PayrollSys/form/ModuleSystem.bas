Public Function Ping(prmIPaddr As String) As Boolean
Dim IPaddr As Long, HopsCount As Long, RTT As Long
Dim MaxHops As Long
    Const SUCCESS = 1
    MaxHops = 15               ' should be enough ...
    IPaddr = inet_addr(prmIPaddr)
    Ping = (GetRTTAndHopCount(IPaddr, HopsCount, MaxHops, RTT) = SUCCESS)
End Function
      'fungsi untuk membuat form windows always on top
      Public Function SetTopMostWindow(hwnd As Long, Topmost As Boolean) _
         As Long

         If Topmost = True Then
            SetTopMostWindow = SetWindowPos(hwnd, HWND_TOPMOST, 0, 0, 0, _
               0, Flags)
         Else
            SetTopMostWindow = SetWindowPos(hwnd, HWND_NOTOPMOST, 0, 0, _
               0, 0, Flags)
            SetTopMostWindow = False
         End If
      End Function

'fungsi untuk Delete File
Public Sub File_Delete(Filename As String)
On Error GoTo ErrorLabel
    Dim filesystemobject As Object
    Set filesystemobject = CreateObject("Scripting.filesystemobject")
    filesystemobject.deletefile Filename, True
    
Exit Sub
ErrorLabel:
    If Err.Number <> 0 Then MsgBox CekError(Err.Number), vbCritical, "LG Error"
    
End Sub
 
'fungsi untuk Copy File Overwrite
Public Sub File_Copy(AsalFolder As String, AsalFile As String, TujuanFolder As String, TujuanFile As String)
    'jika ada cek apakah file sudah ada
    If CekStatusFile(TujuanFile, TujuanFolder) Then
        'Jika ada Maka Hapus File.
        File_Delete (TujuanFolder & TujuanFile)
    End If
        'Copy file ke folde output
        FileCopy AsalFolder & AsalFile, TujuanFolder & TujuanFile
End Sub

'fungsi untuk menutup semua form
Public Sub UnloadAll()
   Dim f As Integer
   f = Forms.Count
   Do While f > 0
       Unload Forms(f - 1)
       If f = Forms.Count Then Exit Do
       f = f - 1
   Loop
End Sub

